<%- model_class = Giris -%>
<div class="page-header">
  <h1><%=t '.title', :default => Giris %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:ilk_basvuru_tarihi) %></th>
      <th><%= model_class.human_attribute_name(:morcati_destegi_aliyor) %></th>
      <th><%= model_class.human_attribute_name(:iletisime_gecen) %></th>
      <th><%= model_class.human_attribute_name(:iletisime_gecme_yolu) %></th>
      <th><%= model_class.human_attribute_name(:notlar) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @giris.each do |giris| %>
      <tr>
        <td><%= giris.id %></td>
        <td><%= giris.ilk_basvuru_tarihi %></td>
        <td><%= giris.morcati_destegi_aliyor %></td>
        <td><%= giris.iletisime_gecen %></td>
        <td><%= giris.iletisime_gecme_yolu %></td>
        <td><%= giris.notlar %></td>
        <td><%=l giris.created_at, format: :long %></td>
        <td>
          <%= link_to giris_path(giris), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to giris_path(giris), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_giris_path,
            :class => 'btn btn-primary' %>
