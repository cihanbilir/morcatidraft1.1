<%- model_class = Giris -%>
<div class="page-header">
  <h1><%=t '.title', :default => Giris %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ilk_basvuru_tarihi) %></dt>
      <dd><pre class="prettyprint"><%= @giris.ilk_basvuru_tarihi %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:morcati_destegi_aliyor) %></dt>
      <dd><pre class="prettyprint"><%= @giris.morcati_destegi_aliyor %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:iletisime_gecen) %></dt>
      <dd><pre class="prettyprint"><%= @giris.iletisime_gecen %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:iletisime_gecme_yolu) %></dt>
      <dd><pre class="prettyprint"><%= @giris.iletisime_gecme_yolu %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:notlar) %></dt>
      <dd><pre class="prettyprint"><%= @giris.notlar %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              giris_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_giris_path(@giris), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              giris_path(@giris),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

